{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
		"allowJs": true,
		"checkJs": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"sourceMap": true,
		"strict": true,
		"module": "esnext",
		"moduleResolution": "bundler",
		"baseUrl": ".",
		"types": ["svelte", "@sveltejs/kit"],
		"paths": {
		  "$lib/*": ["src/lib/*"]
		}
	}
}
