{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
	  "target": "ESNext",
	  "useDefineForClassFields": true,
	  "module": "ESNext",
	  "moduleResolution": "node",
	  "resolveJsonModule": true,
	  "allowJs": true,
	  "checkJs": true,
	  "isolatedModules": true,
	  "types": ["svelte"],
	  "outDir": "./.tsbuild"
	},
	"include": ["src"]
}